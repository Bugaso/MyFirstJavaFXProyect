<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="salPane" layoutY="35.0" prefHeight="329.0" prefWidth="505.0" xmlns="http://javafx.com/javafx/22"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.bugardo.controllers.salidaController" stylesheets="@../../../com.bugardo.css/tablesView.css">
   <children>

      <TableView fx:id="table" editable="false" layoutX="37.0" layoutY="88.0" prefHeight="155.0" prefWidth="281.0">
        <columns>
          <TableColumn fx:id="patColm" editable="false" prefWidth="83.0" resizable="false" text="Patente" reorderable="false"/>
          <TableColumn fx:id="fechaColm" editable="false" prefWidth="131.0" resizable="false" text="Fecha de entrada" reorderable="false" />
            <TableColumn fx:id="pagoColm" editable="false" prefWidth="66.0" resizable="false" text="Pagado" reorderable="false"/>
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>

      <Label layoutX="174.0" layoutY="37.0" prefHeight="24.0" prefWidth="158.0" text="Retirar Vehiculo">
         <font>
            <Font name="Cambria Bold" size="20.0" />
         </font></Label>
      <DatePicker fx:id="salidaDate" disable="true" layoutX="340.0" layoutY="153.0" prefHeight="25.0" prefWidth="120.0" />
      <Spinner fx:id="hourSpin" disable="true" layoutX="340.0" layoutY="178.0" prefHeight="25.0" prefWidth="51.0" />
      <Spinner fx:id="minSpin" disable="true" layoutX="409.0" layoutY="178.0" prefHeight="25.0" prefWidth="51.0" />
      <TextField fx:id="patText" layoutX="340.0" layoutY="88.0" prefHeight="25.0" prefWidth="120.0" />
      <Button fx:id="buscarBtn" layoutX="340.0" layoutY="121.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="120.0" text="Buscar vehiculo" />
      <Button fx:id="sacarBtn" contentDisplay="CENTER" disable="true" layoutX="353.0" layoutY="215.0" mnemonicParsing="false" text="Sacar Vehiculo" />
   </children>
</Pane>
